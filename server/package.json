{
  "name": "mna-partage-simplifie",
  "version": "1.0.0",
  "description": "[MNA] Partage simplifiÃ©",
  "repository": "https://github.com/mission-apprentissage/partage-simplifie.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "exports": "./src/index.js",
  "type": "module",
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "watch": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "cross-env DOTENV_CONFIG_PATH=.env.example PARTAGE_SIMPLIFIE_LOG_LEVEL=fatal mocha --config=mocharc.json tests/integration",
    "test:single": "cross-env DOTENV_CONFIG_PATH=.env.example mocha --config=mocharc.json",
    "cli": "node -r dotenv/config src/jobs/cli.js",
    "coverage": "nyc --temp-dir .coverage/.nyc_output --report-dir .coverage --reporter=lcov --reporter=html yarn test",
    "lint": "eslint src/ tests/",
    "migration:create": "migrate-mongo create",
    "migration:up": "migrate-mongo up",
    "migration:down": "migrate-mongo down"
  },
  "dependencies": {
    "arg": "5.0.2",
    "axios": "0.27.2",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-mongodb-logger": "1.0.0",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "cli-progress": "3.11.0",
    "commander": "9.4.0",
    "cross-env": "7.0.3",
    "crypto": "1.0.1",
    "date-fns": "2.28.0",
    "env-var": "7.1.1",
    "express": "4.18.1",
    "global": "4.4.0",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash.omit": "4.5.0",
    "lodash.omitby": "4.6.0",
    "lodash.throttle": "4.1.1",
    "migrate-mongo": "9.0.0",
    "mockdate": "3.0.5",
    "mongodb": "4.9.0",
    "multer": "1.4.5-lts.1",
    "oleoduc": "0.7.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "sha512crypt-node": "1.0.2",
    "xlsx": "0.18.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@faker-js/faker": "7.5.0",
    "axiosist": "0.10.0",
    "dotenv": "16.0.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "lint-staged": "12.4.1",
    "mocha": "10.0.0",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.9",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "randexp": "0.5.3",
    "supertest": "6.2.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
